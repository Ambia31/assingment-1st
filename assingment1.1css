*{margin:0%;padding:0%;}
body{
    width:auto;
    background-color: aliceblue;
}
.wrap{
    width:80%;
    margin-left: 10%;
    margin-top: 150px;
}
.flexi{
    display: flex;
}
.first-section{
    width:100%;
    font-size: x-small;
}
.halfwidth{
    width:50%;
}
.halfwidth img{
    width:400px;
    float:right;
    margin-top: -150px;
    margin-right: 20px;
}
.color{
    color: rgb(8, 192, 48);
}

#border{
    border-left: 5px solid green;
    border-radius: 5px;
    border-image: linear-gradient(rgb(144, 175, 5), green);
    border-image-slice: 1;
}
h1{
    font-size:45px;
}
h2{
    font-size: 38px;
}
.link-button{
    padding:10px 40px;
    background-color: darkgreen;
    text-decoration: none;
    color:white;
    border-radius: 5px;
    font-size: 20px;
}
p{
    margin-top:40px;
    margin-bottom: 30px;
}


.second-section{
    text-align: center;
    margin-bottom: 70px;
    width: 100%;
    }
    .types{
        text-align: left;
        background-color: rgb(250, 223, 214);
        width: 32%;
        margin-right: 2%;
        font-weight: lighter;
        color:rgb(97, 96, 96);
        border-radius: 10px;
        margin-top: 50px;
        padding:20px;
        font-size: smaller;
    }
    .two{
        background-color: rgb(247, 232, 185);
    }
    
    #last-type{
        margin-right:0;
        background-color: rgb(204, 248, 217);
    }
    img{
        width:100%;
        height: auto;
    }
    .imge{
        height:100px;
        width: 100px;
    }
    h3{
        margin-bottom: 25px;
        margin-right: 5px;
        color:black;
    }
    .box-paragraph{
        font-size: 100%;
        margin-top: -5%;
    }
      .third-section{
        background-image: url(images/shape-1.png);
        background-repeat: no-repeat;
        background-size: 22%;
        background-position:center left;
        font-size: xx-small;
        font-weight: lighter;


    }
    .half{
        width:50%;
    }
    #third-sections-img{
        margin-left: 7%;
        margin-right: 7%;
        width:40%;
    }
    p{
        font-size: 180%;
        font-family: serif;
        word-spacing: 2px;
     }
     .fourth-section{
         margin-top: 10%;
         font-size: x-small;
         font-weight: lighter;
         background-image: url(images/shape-1.png);
         background-repeat: no-repeat;
         background-size: 12%;
         background-position: right bottom;

     }
     #fourth-section-img{
         width:40%;
         margin-top: 0px;
         margin-right:5%;

     }

     .fifth-section{
         margin-left: 12%;
         margin-top: 8%;
         margin-bottom: 5%;
         font-size: x-small;
         font-weight: lighter;
         text-align: center;
     }
     .half-width{
         width:60%;
         margin-top: 5%;
         margin-left:5% ;
         text-align: left;

     }
     #fifth-section-bg{
        background-image: url(images/shape-3.png) ,url(images/shape-3.png), url(images/shape-3.png);
        background-size: 11%, 7%, 7%;
        background-repeat: no-repeat;
        background-position:top left, 21% 70%, 2% 97.5%;
        margin-top: 5%;
        margin-left: -5%;

     }
 
     
     #fift-sections-img{
         width:200px;
         height: auto;
         margin-right: 15%;
         
     }

    footer{
       font-family:Lucida Handwriting;
       background-color: black;
       color:white;
       text-align: center;
       height: 110px;

    }
    .footer-header{
        font-size: xx-large;
        margin-top:-7px;
    }
    .footer-p{
        font-size: smaller;
        margin-top:-13px;
        margin-bottom: 5%;
    }